<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
   android:orientation="vertical"
    android:id="@+id/activity_menu__floor_master"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.yunk_.gisooksa.MenuActivity_FloorMaster">

    <TextView
        android:text="OOO학생 (O동 O층장)"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:id="@+id/textView"
        android:textSize="24sp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_x="69dp"
        android:layout_y="34dp" />


    <Button
        android:text="회원정보 수정"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:id="@+id/access_btn"
        android:layout_alignTop="@+id/view_student_btn"
        android:layout_alignBottom="@+id/view_student_btn"
        android:layout_alignEnd="@+id/out_btn"
        android:layout_x="78dp"
        android:layout_y="326dp"
        android:layout_row="2"
        android:layout_column="1" />

    <Button
        android:text="상벌점 확인"
        android:id="@+id/view_student_btn"
        android:layout_width="119dp"
        android:layout_height="60dp"
        android:layout_x="64dp"
        android:layout_y="260dp"
        android:layout_row="2"
        android:layout_column="0"
        android:layout_marginTop="29dp"
        android:layout_below="@+id/out_btn"
        android:layout_alignStart="@+id/qr_btn" />

    <Button
        android:text="로그아웃"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:id="@+id/resign_btn"

        android:layout_x="230dp"
        android:layout_y="329dp"
        android:layout_row="4"
        android:layout_column="1"
        android:layout_below="@+id/break_view_btn"
        android:layout_alignStart="@+id/access_btn"
        android:layout_marginTop="13dp" />

    <Button
        android:text="QR코드 보기"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:id="@+id/qr_btn"
        android:layout_x="47dp"
        android:layout_y="119dp"
        android:layout_row="1"
        android:layout_column="0"
        android:layout_marginStart="45dp"
        android:layout_marginTop="54dp"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true" />

    <Button
        android:text="외박신청"
        android:id="@+id/out_btn"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_x="217dp"
        android:layout_y="131dp"
        android:layout_row="1"
        android:layout_column="1"
        android:layout_marginStart="20dp"
        android:layout_alignBaseline="@+id/qr_btn"
        android:layout_alignBottom="@+id/qr_btn"
        android:layout_toEndOf="@+id/qr_btn" />

    <TextView
        android:text="현재상태 : 기숙사내부"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/out_btn"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="19dp"
        android:id="@+id/textView55" />

    <Button
        android:text="관리층 학생보기"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:id="@+id/button22"
        android:layout_below="@+id/break_view_btn"
        android:layout_alignStart="@+id/allow_visitor_btn" />

    <Button
        android:text="고장접수"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:id="@+id/break_view_btn"
        android:layout_x="201dp"
        android:layout_y="302dp"
        android:layout_row="3"
        android:layout_column="1"
        android:textAlignment="center"
        android:layout_alignBaseline="@+id/allow_visitor_btn"
        android:layout_alignBottom="@+id/allow_visitor_btn"
        android:layout_alignStart="@+id/resign_btn" />

    <Button
        android:text="상벌점 부여"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginTop="11dp"
        android:id="@+id/allow_visitor_btn"
        android:layout_x="230dp"
        android:layout_y="129dp"
        android:layout_row="3"
        android:layout_column="0"
        android:layout_below="@+id/view_student_btn"
        android:layout_alignEnd="@+id/view_student_btn" />

    <Button
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:id="@+id/button27"
        android:layout_below="@+id/resign_btn"
        android:layout_alignStart="@+id/view_student_btn"
        android:text="외박신청내용" />
</RelativeLayout>
